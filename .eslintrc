{
  "parser": "babel-eslint",
  "extends": [
    "vue"
  ],
  "plugins": [
    "babel"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "__DEV__": false,
    "__PROD__": false,
    "__MOCK__": false,
    "__DEBUG__": false,
    "__COVERAGE__": false,
    "__BASENAME__": false
  },
  "rules": {
    "semi": [2, "never"],
    "generator-star-spacing": 0,
    "space-before-function-paren": 0
    // "generator-star-spacing": 1
  }
}
